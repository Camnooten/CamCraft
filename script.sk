on join:
	if {players.online} is not set:
		set {players.online} to 1
	else:
		add 1 to {players.online}

on quit:		
	subtract 1 from {players.online}
every 20 seconds:
		loop all players:
				if loop-player's world is "world":
				wipe loop-player's sidebar
				set name of sidebar of loop-player to "&6&lCamCraft"
				set score "&e&lCoins:" in sidebar of loop-player to 15
				set score "&6%{coins.%loop-player%}%" in sidebar of loop-player to 14
				set score "&1 " in sidebar of loop-player to 13
				set score "&cRank&f: " in sidebar of loop-player to 12
				set score "%{rank.%loop-player%}%" in sidebar of loop-player to 11
				else:
				if loop-player is op:
					set score "&f&lOP (Every rank)" in sidebar of loop-player to 11
				set score "&6 " in sidebar of loop-player to 10
				set score "&7Player&f: " in sidebar of loop-player to 9
				set score "&d%loop-player%" in sidebar of loop-player to 8
				set score "&d " in sidebar of loop-player to 7
				set score "&aOnline:" in sidebar of loop-player to 6
				set score "%number of all players%/2500" in sidebar of loop-player to 5
				set score "&cPing: %loop-players ping%" in sidebar of loop-player to 4
				set score "&eWebsite&f: https://goo.gl/PczWvz" in sidebar of loop-player to 3
	
				
command /msg <player> <text>:
	description: Whisper another player
	usage: /msg <player> <message>
	aliases: /m
	trigger:
		set {chat.private.reply.%player%} to arg 1
		set {chat.private.reply.%arg 1%} to player
		send "&c[MSG] &aTO: %arg 1% &e->&c %arg 2%" to player
		send "&c[MSG] &eFROM: %player% &e->&c %arg 2%" to arg 1
command /time <text>:
	permission: cc.time
	trigger:
		if arg 1 is "day":
			execute console command "minecraft:time set day"
			message "&6Set time to day"
		if arg 1 is "night":
			execute console command "minecraft:time set night"
			message "&6Set time to night"
		
		
		
command /reply <text>:	
	description: Reply command.
	usage: /reply <message>
	aliases: /r
	trigger:
		send "&c[MSG] &aTO: %{chat.private.reply.%player%}% &e->&c %arg 1%" to player
		send "&c[MSG] &eFROM: %player% &e->&c %arg 1%" to {chat.private.reply.%player%}
		
		
