			set {tabname.rank.yt.%player%} to false
			stop
		if arg 2 is "Developer":
			execute console command "manuadd %arg 1% Developer"
			send "&6&lCamCraft&7>&9Your rank has been updated to Developer!" to arg 1
			set {_p} tab name to "{@DEV} &7%arg-1%"
			set {tabname.rank.owner.%player%} to false
			set {tabname.rank.admin.%player%} to false
			set {tabname.rank.player.%player%} to false
			set {tabname.rank.builder.%player%} to false
			set {tabname.rank.moderator.%player%} to false
			set {tabname.rank.dev.%player%} to true
			set {tabname.rank.hb.%player%} to false
			set {tabname.rank.yt.%player%} to false
			stop
		if arg 2 is "HB":
			execute console command "manuadd %arg 1% HB"
			send "&6&lCamCraft&7>&9Your rank has been updated to HB!" to arg 1
			set {_p} tab name to "{@HB} &7%arg-1%"
			set {tabname.rank.owner.%player%} to false
			set {tabname.rank.admin.%player%} to false
			set {tabname.rank.player.%player%} to false
			set {tabname.rank.moderator.%player%} to false
