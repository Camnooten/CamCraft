command /rank: 
 permission: cc.rank
 permission message: {@denied}
 aliases: /rank-help, /rankhelp, /rankshelp
 trigger: 
  message "&6Rank Sub-Commands:"
  message "&b/rank-update <player> <rank> &6- &dUpdates their rank."
  message "&b/rank-check <player> &6- &dChecks their rank."
  
  
command /rank-reload:
 permission: cc.rank
 permission message: {@denied}
 trigger:
  make player execute command "/manload"
  message "&6&lCamCraft&7>&9All ranks reloaded."
  

			
command /rank-create <text>:
	permission: cc.rank
	permission message: {@denied}
	trigger:
		message "&6&lCamCraft&7> Creating rank %arg 1% via &bGroupManager &7in &a3 &8seconds..."
		message "&6%arg 1%: &9No permsissions when created."
		wait 3 seconds
		execute console command "mangadd %arg 1%"
		message "&6&lCamCraft&7> Created group %arg 1%"
		execute console command "mangaddv %arg 1% . prefix"
command /ranks:
	permission: cc.rank
	permission message: {@denied}.
	trigger:
		make player execute command "/listgroups"
		
command /rank-toggleautosave:
	permission: cc.rank
	permission message: {@denied}
	trigger:
		message "&6&lCamCraft&7> Toggled autosave"
		make player execute command "/mantogglesave"
		
command /rank-addperm <text> <text>:
	permission: cc.rank
	permission message: {@denied}
	trigger:
		make player execute command "/mangaddp %arg 1% %arg 2%"
		message "&6&lCamCraft&7> Added permission %arg 2% to %arg 1%"
		
command /award <text> <text>:
	if arg 1 is "cuberunner":
		add 150 to {coins.%arg-1%}
		message "You have recieved 150 coins for winning the game"
		
		
