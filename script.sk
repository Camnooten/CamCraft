command /cos:
	trigger:
		open chest with 1 rows named "&6&lCosmetics" to player
		wait 1 tick
		format slot 1 of player with head named "&6&lEmotes" with lore "&cWear cool emotes!" to close then run "/buy emotes"
on damage:
	if victim is player:
		cancel event
command /disablerain:
	execute console command "/toggledownfall"
