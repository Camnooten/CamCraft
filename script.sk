command /rank-update <offline player> <text>:
	permission: cc.rank
	aliases: /rank-set 
	permission message: &6&lCamCraft&7>&c&lInvalid permission.
	trigger:
		set {_p} to arg 1
		if arg 2 is "Owner":
			execute console command "manuadd %arg 1% Owner"
			send "&6&lCamCraft&7>&9Your rank has been updated to Owner!" to arg 1
			set {_p} tab name to "{@OWNER} &7%arg-1%"
			set {tabname.rank.owner.%player%} to true
			set {tabname.rank.admin.%player%} to false
			set {tabname.rank.player.%player%} to false
			set {tabname.rank.moderator.%player%} to false
			set {tabname.rank.dev.%player%} to false
			set {tabname.rank.hb.%player%} to false
			set {tabname.rank.builder.%player%} to false
			set {tabname.rank.yt.%player%} to false
			stop
		if arg 2 is "Admin":
			execute console command "manuadd %arg 1% Admin"
			send "&6&lCamCraft&7>&9Your rank has been updated to Admin!" to arg 1
			set {_p} tab name to "{@ADMIN} &7%arg-1%"
			set {tabname.rank.owner.%player%} to false
			set {tabname.rank.admin.%player%} to true
			set {tabname.rank.player.%player%} to false
			set {tabname.rank.moderator.%player%} to false
			set {tabname.rank.builder.%player%} to false
			set {tabname.rank.dev.%player%} to false
			set {tabname.rank.hb.%player%} to false
			set {tabname.rank.yt.%player%} to false
			stop
		if arg 2 is "Player":
