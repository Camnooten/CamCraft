			send "&6&lCamCraft&7>&9Your rank has been updated to Builder!" to arg 1
			set {_p} tab name to "{@BUILDER} &7%arg-1%"
			set {tabname.rank.owner.%player%} to false
			set {tabname.rank.admin.%player%} to false
